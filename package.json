{
  "name": "global-digital-health-vault",
  "version": "1.0.0",
  "description": "Secure, universal health record platform enabling global medical data sharing with consent control",
  "main": "src/server/index.js",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "server:dev": "nodemon src/server/index.js",
    "client:dev": "vite",
    "build": "vite build",
    "start": "node src/server/index.js",
    "test": "vitest",
    "lint": "eslint src/",
    "format": "prettier --write src/"
  },
  "keywords": [
    "healthcare",
    "EHR",
    "FHIR",
    "DICOM",
    "medical-records",
    "blockchain",
    "encryption",
    "consent-management"
  ],
  "author": "Digital Health Vault Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^8.0.3",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "crypto": "^1.0.1",
    "helmet": "^7.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "multer": "^1.4.5-lts.1",
    "fhir": "^4.11.1",
    "dicom-parser": "^1.8.21",
    "web3": "^4.2.1",
    "node-forge": "^1.3.1",
    "axios": "^1.6.2",
    "socket.io": "^4.7.4",
    "redis": "^4.6.10",
    "sharp": "^0.33.0",
    "@tensorflow/tfjs-node": "^4.15.0"
  },
  "devDependencies": {
    "vite": "^5.0.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "@vitejs/plugin-react": "^4.2.0",
    "tailwindcss": "^3.3.6",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2",
    "vitest": "^1.0.4",
    "eslint": "^8.55.0",
    "prettier": "^3.1.1",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}